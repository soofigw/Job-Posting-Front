ont-weight: 700;\r\n  color: var(--emp-text);\r\n  margin: 0 0 4px;\r\n  line-height: 1.3;\r\n}\r\n\r\n.vacante-company {\r\n  font-size: 14px;\r\n  font-weight: 600;\r\n  color: var(--primary);\r\n  margin: 0 0 10px;\r\n}\r\n\r\n.vacante-location {\r\n  font-size: 13px;\r\n  color: var(--emp-text-soft);\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 6px;\r\n  margin-bottom: 14px;\r\n}\r\n\r\n.vacante-location::before {\r\n  content: \"\uD83D\uDCCD\";\r\n  font-size: 13px;\r\n}\r\n\r\n/* BADGES (MODALIDAD) */\r\n.badge-mode {\r\n  display: inline-block;\r\n  font-size: 12px;\r\n  font-weight: 700;\r\n  padding: 6px 14px;\r\n  border-radius: 999px;\r\n}\r\n\r\n.badge-mode.hybrid {\r\n  background: #f1edff;\r\n  color: #6b5cff;\r\n}\r\n\r\n.badge-mode.remote {\r\n  background: #e6f4ff;\r\n  color: #1976d2;\r\n}\r\n\r\n.badge-mode.onsite {\r\n  background: #e9f8ef;\r\n  color: #1b8f4b;\r\n}\r\n\r\n/* EMPTY STATE */\r\n.no-vacantes {\r\n  color: var(--emp-text-soft);\r\n  padding: 20px 0;\r\n}\r\n\r\n/* =========================================\r\n   MIS POSTULACIONES\r\n========================================= */\r\n\r\n.applications-list {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 15px;\r\n}\r\n\r\n.app-card {\r\n  background: var(--emp-card);\r\n  padding: 25px;\r\n  border-radius: 16px;\r\n  border: 1px solid var(--emp-border-light);\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  transition: all 0.2s ease;\r\n}\r\n\r\n.app-card:hover {\r\n  transform: translateY(-2px);\r\n  box-shadow: 0 5px 20px rgba(0,0,0,0.05);\r\n  border-color: #ffdce0;\r\n}\r\n\r\n.app-main-info {\r\n  display: flex;\r\n  gap: 20px;\r\n  align-items: center;\r\n}\r\n\r\n.app-logo {\r\n  width: 55px;\r\n  height: 55px;\r\n  border-radius: 10px;\r\n  object-fit: cover;\r\n  border: 1px solid #eee;\r\n}\r\n\r\n.app-title {\r\n  font-size: 18px;\r\n  font-weight: 700;\r\n  color: var(--emp-text);\r\n  margin: 0 0 4px 0;\r\n}\r\n\r\n.app-company {\r\n  font-size: 15px;\r\n  color: var(--primary);\r\n  font-weight: 600;\r\n  margin: 0 0 8px 0;\r\n}\r\n\r\n.app-meta {\r\n  display: flex;\r\n  gap: 15px;\r\n  font-size: 13px;\r\n  color: var(--emp-text-soft);\r\n}\r\n\r\n/* STATUS BADGES */\r\n.status-badge {\r\n  display: inline-flex;\r\n  align-items: center;\r\n  gap: 6px;\r\n  padding: 6px 12px;\r\n  border-radius: 20px;\r\n  font-size: 13px;\r\n  font-weight: 600;\r\n}\r\n\r\n.sent { background: #e3f2fd; color: #1976d2; }\r\n.seen { background: #f3e5f5; color: #7b1fa2; }\r\n.process { background: #e8f5e9; color: #2e7d32; }\r\n.rejected { background: #ffebee; color: #c62828; }\r\n.finalist { background: #fff8e1; color: #f57f17; }\r\n\r\n/* =========================================\r\n   MIS FAVORITOS\r\n========================================= */\r\n\r\n.favorites-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));\r\n  gap: 20px;\r\n}\r\n\r\n.fav-card {\r\n  background: white;\r\n  border: 1px solid var(--emp-border-light);\r\n  border-radius: 16px;\r\n  padding: 20px;\r\n  transition: 0.2s;\r\n}\r\n\r\n.fav-card:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: 0 10px 25px rgba(0,0,0,0.08);\r\n  border-color: #ffdce0;\r\n}\r\n\r\n.btn-text-red {\r\n  background: none;\r\n  border: none;\r\n  color: var(--primary); /* Usa el rojo de la marca (#FF5A5F) */\r\n  font-size: 14px;\r\n  font-weight: 600;\r\n  cursor: pointer;\r\n  text-decoration: none;\r\n  padding: 0;\r\n  transition: color 0.2s;\r\n}\r\n\r\n.btn-text-red:hover {\r\n  color: var(--primary-hover); /* Rojo más oscuro al pasar el mouse */\r\n  text-decoration: underline;\r\n}\r\n\r\n.app-meta span { \r\n  display: flex; \r\n  align-items: center; \r\n  gap: 5px; \r\n}\r\n\r\n.app-status-col {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: flex-end; \r\n  min-width: 160px;\r\n}\r\n\r\n@media (max-width: 700px) {\r\n  .app-card { fl